        <StackPanel Background="#FFE1E2FF" Grid.Row="0">
            <Image Source="Resources/beauty_logo.png" Width="50" Height="50" HorizontalAlignment="Left" Margin="5"/>
        </StackPanel>
        <StackPanel Background="#FFE1E2FF" Grid.Row="2">

        </StackPanel>
        <Frame x:Name="frame" Width="auto" LoadCompleted="frame_LoadCompleted" Grid.Row="1"/>

    </Grid>

</Window>
